{
    "name": "BlackCat",
    "description": "BlackCat decreases the brightness of the web pages you visit.",
    "version": "1.0",
    "permissions": [
        "<all_urls>"
    ],
    "browser_action": {
        "default_title": "Toggle dark mode",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "manifest_version": 2
}